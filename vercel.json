{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/public/Images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}